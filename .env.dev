# Database Modeling Tool - 개발 환경 설정

# Spring Boot 설정
SPRING_PROFILES_ACTIVE=dev
PORT=8080

# 데이터베이스 설정 (개발용)
DB_URL=jdbc:postgresql://localhost:5432/dbmodeling_dev
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_MAX_POOL_SIZE=10
DB_MIN_IDLE=5

# 로깅 설정
SHOW_SQL=true
HIBERNATE_STATS=false

# CORS 설정
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# PostgreSQL 컨테이너 설정
POSTGRES_DB=dbmodeling_dev
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# pgAdmin 설정
PGADMIN_DEFAULT_EMAIL=admin@dbmodeling.com
PGADMIN_DEFAULT_PASSWORD=admin123