# Database Modeling Tool - 테스트 환경 설정

# Spring Boot 설정
SPRING_PROFILES_ACTIVE=test
PORT=8081

# 데이터베이스 설정 (테스트용)
DB_URL=jdbc:postgresql://localhost:5433/dbmodeling_test
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_MAX_POOL_SIZE=5
DB_MIN_IDLE=1

# 로깅 설정
SHOW_SQL=false
HIBERNATE_STATS=false

# PostgreSQL 컨테이너 설정
POSTGRES_DB=dbmodeling_test
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres